<%-include('partials/header.ejs')%>
	

    <% if (users && users.length > 0){%>
   

	<!--	<div class = "container-fluid h-100 text-center">-->
		<div class = "row top5">
        <% users.forEach(function(user){%>
        <div class = "question col-12 my-auto text-center border border-primary">
          <span class = "mb-5 border">
          <h4>Username: <%=user.username%></h4>
		  <h4>First Name: <%=user.first_name%></h4>
			<h4>Last Name: <%=user.last_name%></h4> 
			<h4>City: <%=user.city%></h4> 
			<h4>State: <%=user.state%></h4> 
			<h4>Country: <%=user.country%></h4>
            <h4>Profile: <%=user.uprofile%></h4>
            <h4>Status: <%=user.user_status%></h4>
         
          </span>
        <div class = "col-12">
            <a href="/users/<%=user.username%>/questions">View Your Questions</a>
        </div>
        <div class = "col-12"></div>
            <a href="/users/<%=user.username%>/answers">View Your Answers</a>
        </div>
        </div>
        

        <%})}%>
 
    

		</div>

		<!--	</div>-->


</div>
<!--<script type="text/javascript" src="/javascript/filter.js"></script>-->
</body>
</html>
